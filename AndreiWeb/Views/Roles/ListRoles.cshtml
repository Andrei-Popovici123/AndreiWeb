@model List<Roles>



<!DOCTYPE html>

<html>
<head>
    <title>title</title>
</head>
<body>
<div>
    <h1>Category List</h1>
    <table class="table table-bordered table-striped">
        <thead>
        <tr>
            <th>
                Category Name
            </th>
            <th>
                ID
            </th>
        </tr>
        </thead>
        <tbody>
        @foreach (var obj in Model.OrderBy(o=> o.Id).Select(o=> new { Id = "[Redacted]", Name = o.Name.ToString()}))
        {
            <tr>
                <td>
                    @obj.Name
                </td>

                <td>
                    @obj.Id
                </td>
            </tr>
        }
        </tbody>
    </table>
</div>
</body>
</html>